<% include top %>
    <form action="/notesave" method="POST">
        <input type="hidden" name="id" value="<%= id? id : ""%>"/>
        <p><label for="title">Title: </label><input type="text" name="title" id="title" value="<%= note? note.title : ""%>"/></p>
        <textarea name="body" rows="10" cols="50"><%= note? note.body : "" %></textarea><br/>
        <input type="submit" name="submit"/> <a href="<%= id? ("/noteview?id=" + id) : "/"%>">Cancel</a>
    </form>
<% include bottom %>
